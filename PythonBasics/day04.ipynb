   "source": [
    "#类的本质：在一堆函数之间传递参数\n",
    "#类的示范\n",
    "class Counter:\n",
    "    '\"一台可以加减的计算器\"'\n",
    "    def _init_(self,a,b):\n",
    "        '\"公共变量\"'\n",
    "        self.a = a\n",
    "        self.b = b\n",
    "\n",
    "    def add(self):\n",
    "        '\"加法\"'\n",
    "        return self.a + self.b\n",
    "\n",
    "    def sub(self):\n",
    "        '\"减法\"'\n",
    "        return self.a - self.b"
   ]
